{
  "name": "react-playground",
  "version": "0.0.0",
  "description": "My React playground",
  "main": "./src/index.js",
  "scripts": {
    "setup": "npm install && npm run validate",
    "validate": "npm-run-all --parallel test:coverage --serial check-coverage",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run validate",
    "build": "webpack --env.dev",
    "prebuild:prod": "npm run clean && npm run validate",
    "build:prod": "webpack --env.prod",
    "stylelint": "stylelint './src/**/*.s?(a|c)ss' --syntax scss",
    "eslint": "eslint ./src",
    "lint": "npm run stylelint && npm run eslint",
    "test:coverage": "echo 'test:coverage: temporary disabled'",
    "check-coverage": "echo 'check:coverage: temporary disabled'",
    "test:watch": "mocha -w --opts ./mocha.opts './test/**/*.spec.js'",
    "test:pattern": "mocha -w --opts ./mocha.opts './test/**/*.spec.js' --grep $pattern",
    "test": "mocha --opts ./mocha.opts './test/**/*.spec.js'",
    "start": "webpack-dev-server --env.dev --port 3333 --progress --inline --hot --content-base src --history-api-fallback"
  },
  "config": {
    "ghooks": {
      "pre-commit": "node_modules/.bin/opt --in pre-commit --exec \"npm run validate\""
    }
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6"
  },
  "keywords": [
    "react",
    "webpack2"
  ],
  "author": "Team Sirius Yoonir",
  "homepage": "https://github.com/leifoolsen/my-playground",
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.5.0",
    "copy-webpack-plugin": "4.0.0",
    "css-loader": "0.25.0",
    "deep-freeze-es6": "1.0.1",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.0",
    "ignore-styles": "5.0.1",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.8.1",
    "mocha": "3.1.2",
    "node-sass": "3.10.1",
    "npm-run-all": "3.1.1",
    "opt-cli": "1.5.1",
    "postcss-loader": "1.0.0",
    "precss": "1.4.0",
    "resolve-url-loader": "1.6.0",
    "rimraf": "2.5.4",
    "run-all": "1.0.1",
    "sass-loader": "4.0.2",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "stylelint": "7.5.0",
    "stylelint-config-standard": "14.0.0",
    "stylelint-webpack-plugin": "0.4.0",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.9"
  }
}
