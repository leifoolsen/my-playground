{
  "name": "es2015-katas",
  "version": "0.0.1",
  "description": "ES2015 katas",
  "author": "Leif Olsen <leif.o.olsen.at.gmail.com> (https://github.com/leifoolsen)",
  "private": true,
  "main": "webpack.config.babel.js",
  "scripts": {
    "setup": "npm install && npm run validate",
    "validate": "npm-run-all --parallel validate-webpack:* test:coverage --serial check-coverage",
    "validate-webpack:dev": "./node_modules/.bin/webpack-validator webpack.config.babel.js --env.dev",
    "validate-webpack:prod": "./node_modules/.bin/webpack-validator webpack.config.babel.js --env.prod",
    "clean": "./node_modules/.bin/rimraf dist",
    "prebuild": "npm run clean && npm run validate",
    "build": "./node_modules/.bin/webpack --env.dev",
    "prebuild:prod": "npm run clean && npm run validate",
    "build:prod": "./node_modules/.bin/webpack --env.prod",
    "stylelint": "./node_modules/.bin/stylelint './src/**/*.s?(a|c)ss' --syntax scss",
    "eslint": "./node_modules/.bin/eslint ./src",
    "lint": "npm run stylelint && npm run eslint",
    "test:coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/istanbul cover --root ./src ./node_modules/.bin/_mocha -- --opts ./mocha.opts  './test/**/*.spec.js'",
    "check-coverage": "./node_modules/.bin/istanbul check-coverage --statements 50 --branches 50 --functions 50 --lines 50",
    "test:watch": "./node_modules/.bin/mocha  -w --opts ./mocha.opts './test/**/*.spec.js'",
    "test:pattern": "./node_modules/.bin/mocha  -w --opts ./mocha.opts './test/**/*.spec.js' --grep $pattern",
    "test": "./node_modules/.bin/mocha  --opts ./mocha.opts './test/**/*.spec.js'",
    "start": "./node_modules/.bin/webpack-dev-server --env.dev --port 8084 --progress --content-base src"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6"
  },
  "dependencies": {
    "moment": "2.13.0"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015-webpack": "6.4.1",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "eslint": "3.0.0",
    "eslint-loader": "1.4.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "ghooks": "1.3.0",
    "html-webpack-plugin": "2.22.0",
    "ignore-styles": "4.0.0",
    "imports-loader": "0.6.5",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.4.0",
    "jsdomify": "2.1.0",
    "mocha": "2.5.3",
    "node-sass": "3.8.0",
    "npm-run-all": "2.3.0",
    "opt-cli": "1.5.1",
    "postcss-loader": "0.9.1",
    "resolve-url-loader": "1.4.4",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.3",
    "style-loader": "0.13.1",
    "stylelint": "6.8.0",
    "stylelint-config-standard": "10.0.0",
    "stylelint-webpack-plugin": "0.2.0",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.13",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-validator": "2.2.2"
  },
  "keywords": [
    "Webpack2",
    "JavaScript",
    "ES6",
    "ES2015",
    "ECMASCRIPT 2015",
    "Babel",
    "SASS",
    "SASS Boilerplate"
  ],
  "license": "MIT"
}
