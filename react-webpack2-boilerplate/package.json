{
  "name": "react-webpack2-boilerplate",
  "version": "0.0.1",
  "description": "React with Webpack2 boilerplate",
  "author": "Leif Olsen <leif.o.olsen.at.gmail.com> (https://github.com/leifoolsen)",
  "main": "webpack.config.babel.js",
  "engineStrict": true,
  "engines": {
    "node": ">= 6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leifoolsen/webpack2-boilerplate.git"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "harmony-reflect": "1.5.1",
    "intl": "1.2.5",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "body-parser": "1.17.2",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-compat": "1.0.3",
    "eslint-plugin-react": "7.0.1",
    "exports-loader": "0.6.4",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.4",
    "husky": "0.13.4",
    "ignore-styles": "5.0.1",
    "imports-loader": "0.7.1",
    "ip": "1.1.5",
    "jsdom": "11.0.0",
    "material-design-icons": "3.0.1",
    "node-http-ping": "0.2.2",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.2",
    "postcss-browser-reporter": "0.5.0",
    "postcss-loader": "2.0.5",
    "postcss-reporter": "4.0.0",
    "precss": "1.4.0",
    "raw-loader": "0.5.1",
    "react-hot-loader": "next",
    "resolve-url-loader": "2.0.2",
    "rimraf": "2.6.1",
    "roboto-fontface": "0.7.0",
    "sass-loader": "6.0.5",
    "style-loader": "0.18.2",
    "stylelint": "7.11.0",
    "stylelint-config-standard": "16.0.0",
    "stylelint-webpack-plugin": "0.7.0",
    "uncss-webpack-plugin": "0.1.1",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-bundle-size-analyzer": "2.7.0",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "keywords": [
    "Webpack2",
    "Webpack",
    "JavaScript",
    "ES6",
    "ES2015",
    "ECMASCRIPT 2015",
    "ES7",
    "ES2016",
    "ECMASCRIPT 2016",
    "Babel",
    "SASS",
    "SASS Boilerplate",
    "Mocha",
    "Chai",
    "Istanbul",
    "Node Express",
    "Cucumber",
    "WebdriverIO"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist rimraf coverage",
    "setup": "npm run clean && npm install",
    "postsetup": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 selenium-standalone install",
    "validate": "npm-run-all lint test:unit",
    "analyze": "webpack --env.prod --json | webpack-bundle-size-analyzer",
    "precommit": "npm run lint",
    "lint": "npm-run-all lint:*",
    "lint:stylelint": "stylelint \"./src/**/*.s?(a|c)ss\" --syntax scss",
    "lint:eslint": "eslint ./src ./server ./test --max-warnings 10",
    "build:dll": "webpack --config webpack.config.dll.js",
    "prebuild": "npm-run-all clean lint build:dll -- --env.dev",
    "build": "cross-env NODE_ENV=development babel-node ./node_modules/.bin/webpack --progress",
    "prebuild:prod": "npm-run-all clean lint -- --env.prod",
    "build:prod": "cross-env NODE_ENV=production babel-node ./node_modules/.bin/webpack --progress",
    "server": "cross-env NODE_ENV=production babel-node ./server",
    "start": "babel-node ./server --hot"
  }
}
