{
  "name": "react-get-started",
  "version": "0.0.1",
  "description": "Get started with React",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "npm run server & npm run watch-frontend",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --progress --profile --colors",
    "watch-frontend": "./node_modules/.bin/webpack-dev-server --hot --inline --module-bind --progress --color",
    "watch-server": "./node_modules/.bin/nodemon ./api/express-server.js --watch api/ --ignore api/data/ --exec ./node_modules/.bin/babel-node --presets es2015,stage-0",
    "server": "./node_modules/.bin/babel-node ./api/express-server.js --presets es2015,stage-0",
    "start": "echo \"Error: no start specified\" && exit 1",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react"
  ],
  "author": "LOL",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel-cli": "^6.3.13",
    "babel-core": "^6.3.13",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "body-parser": "^1.14.1",
    "config": "^1.17.1",
    "cookie-parser": "^1.4.0",
    "css-loader": "^0.23.0",
    "es6-promise": "^3.0.2",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "morgan": "^1.6.1",
    "multer": "^1.1.0",
    "node-http-proxy": "^0.2.3",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.8.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "winston": "^2.1.1"
