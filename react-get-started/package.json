{
  "name": "react-get-started",
  "version": "0.0.1",
  "description": "Get started with React",
  "author": "Leif Olsen <leif.o.olsen.at.gmail.com> (https://github.com/leifoolsen)",
  "main": "./src/index.js",
  "scripts": {
    "setup": "npm install && npm run validate",
    "validate": "npm-run-all --serial test",
    "validate-webpack:dev": "./node_modules/.bin/webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "./node_modules/.bin/webpack-validator webpack.config.js --env.prod",
    "clean": "./node_modules/.bin/rimraf dist",
    "prebuild": "npm run clean && npm run validate",
    "build": "./node_modules/.bin/webpack --env.dev",
    "prebuild:prod": "npm run clean && npm run validate",
    "build:prod": "./node_modules/.bin/webpack --env.prod",
    "stylelint": "./node_modules/.bin/stylelint './src/**/*.s?(a|c)ss' --syntax scss",
    "eslint": "./node_modules/.bin/eslint ./src",
    "lint": "npm run stylelint && npm run eslint",
    "test:coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/istanbul cover --root ./src ./node_modules/.bin/_mocha -- --opts ./mocha.opts  './test/**/*.spec.js'",
    "check-coverage": "./node_modules/.bin/istanbul check-coverage --statements 50 --branches 50 --functions 50 --lines 50",
    "test:watch": "./node_modules/.bin/mocha  -w --opts ./mocha.opts './test/**/*.spec.js'",
    "test:pattern": "./node_modules/.bin/mocha  -w --opts ./mocha.opts './test/**/*.spec.js' --grep $pattern",
    "test": "./node_modules/.bin/mocha  --opts ./mocha.opts './test/**/*.spec.js'",
    "analyze": "./node_modules/.bin/webpack --env.prod --json | ./node_modules/.bin/webpack-bundle-size-analyzer",
    "start": "./node_modules/.bin/webpack-dev-server --env.dev --port 8088 --progress --content-base src"
  },
  "keywords": [
    "react",
    "webpack2"
  ],
  "dependencies": {
    "material-design-lite": "1.2.0",
    "mdl-ext": "0.9.20",
    "react": "15.3.0",
    "react-dom": "15.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "eslint": "3.2.2",
    "eslint-loader": "1.5.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.0.0",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "0.9.0",
    "ghooks": "1.3.2",
    "html-webpack-plugin": "2.22.0",
    "ignore-styles": "4.0.0",
    "imports-loader": "0.6.5",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.4.2",
    "jsdomify": "2.1.0",
    "material-design-icons": "2.2.3",
    "mocha": "3.0.2",
    "node-sass": "3.8.0",
    "npm-run-all": "2.3.0",
    "opt-cli": "1.5.1",
    "postcss-loader": "0.9.1",
    "resolve-url-loader": "1.6.0",
    "rimraf": "2.5.4",
    "roboto-fontface": "0.6.0",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "stylelint": "7.1.0",
    "stylelint-config-standard": "12.0.0",
    "stylelint-webpack-plugin": "0.3.0",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.20",
    "webpack-bundle-size-analyzer": "2.0.2",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-validator": "2.2.7"
  },
  "license": "MIT",
  "private": true
}
